<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dp"/>
            <gradient android:startColor="#ff732b" android:endColor="#ff732b" android:angle="270" android:centerY="0.75" android:centerColor="#ff732b"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#80ff7433" android:endColor="#a0ff7433" android:angle="270" android:centerY="0.75" android:centerColor="#80ff7433"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5dp"/>
                <gradient android:startColor="#10c3a8" android:endColor="#10c3a8" android:angle="270"/>
            </shape>
        </clip>
    </item>
</layer-list>
